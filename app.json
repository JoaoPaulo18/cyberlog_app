{
  "expo": {
    "name": "CyberLog",
    "slug": "cyberlog_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconeApp.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/iconeApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.draxS2.cyberlog_app",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "background-fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "versionCode": 2,
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "minSdkVersion": 24,
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconeApp.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.POST_NOTIFICATIONS",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "package": "com.draxS2.cyberlog_app",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/iconeApp.png"
    },
    "plugins": [
      "expo-audio",
      [
        "expo-notifications",
        {
          "icon": "./assets/iconeApp.png",
          "color": "#ffffff"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "extra": {
      "oneSignalAppId": "d0b1c8f2-3a6e-4f5b-9c7d-2f8e1b3c4d5e",
      "eas": {
        "projectId": "9115a4e5-3a1a-4573-a3a5-5439f01a1b67"
      }
    },
    "owner": "draxs2"
  }
}
